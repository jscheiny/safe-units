{
    "extends": "../../tsconfig.base.json",
    "compilerOptions": {
        "declaration": true,
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "noImplicitThis": true,
        "alwaysStrict": true,
        "noUnusedLocals": false,
        "skipLibCheck": true,
        "target": "es5",
        "lib": [
            "es5",
            "es2015",
            "es2015.core"
        ],
        "baseUrl": ".",
        "paths": { "safe-units": ["../../src"] }
    }
}